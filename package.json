{
  "name": "ts-rest-pg",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Base app restapi with pg",
  "author": "Hisoft Vietnam",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && npx tsc-alias && yarn run copyfile && yarn run copykey",
    "db:seed": "ts-node node_modules/typeorm-seeding/dist/cli.js -n ./src/database/ormconfig.ts seed",
    "db:config": "ts-node node_modules/typeorm-seeding/dist/cli.js -n ./src/database/ormconfig.ts config",
    "copyfile": "copyfiles -f src/**/*.ejs dist/templates",
    "copykey": "cp -r src/utils/keys dist/utils",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "yarn test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dbml/core": "^2.4.2",
    "@sendgrid/mail": "^7.6.2",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.7",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "fast-json-stable-stringify": "^2.1.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.3.2",
    "passport": "^0.5.2",
    "twilio": "^4.14.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.197",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "bcrypt": "^5.0.1",
    "class-validator-jsonschema": "^3.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.7",
    "envalid": "^7.3.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "husky": "^8.0.0",
    "jest": "27.4.x",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.0.0",
    "morgan": "^1.10.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "prettier": "^2.6.2",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.3",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.6.2",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  }
}
